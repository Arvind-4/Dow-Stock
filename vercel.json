{
    "version": 2,
    "builds": [
      {
        "src": "backend/app/main.py",
        "use": "@vercel/python"
      },
      { 
        "src": "package.json", 
        "use": "@vercel/client" 
      }
    ],
    "routes": [
      {
        "src": "/api",
        "dest": "backend/app/main.py"
      },
      {
        "src": "/api/stocks",
        "dest": "backend/app/main.py"
      }
    ]
  }